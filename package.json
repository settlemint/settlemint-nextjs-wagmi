{
  "name": "settlemint-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "check": "biome ci --organize-imports-enabled=false src/ && bunx tsc",
    "lint": "biome lint --write --unsafe src/",
    "format": "biome format --write src/",
    "codegen": "bun scripts/codegen.ts",
    "predev": "bun codegen",
    "prebuild": "bun codegen",
    "ci": "next build",
    "docker": "docker buildx build --platform linux/amd64,linux/arm64 -t settlemint/settlemint-eas-coffee:main-8 --push .",
    "docker:run": "docker run -p 3000:3000 settlemint/settlemint-eas-coffee"
  },
  "dependencies": {
    "@ethereum-attestation-service/eas-sdk": "^2.6.0",
    "@tanstack/react-query": "5.51.24",
    "connectkit": "1.8.2",
    "encoding": "0.1.13",
    "ethers": "^6.13.2",
    "graphql-request": "^7.1.0",
    "leaflet": "^1.9.4",
    "next": "14.2.5",
    "nominatim-browser": "^2.1.0",
    "openapi-fetch": "0.11.1",
    "path-to-regexp": "7.1.0",
    "pino-pretty": "11.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-leaflet": "^4.2.1",
    "react-tooltip": "^5.28.0",
    "tailwind-scrollbar": "^3.1.0",
    "wagmi": "2.12.6"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/bun": "1.1.6",
    "@types/node": "22.4.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "openapi-typescript": "7.3.0",
    "postcss": "8.4.41",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4"
  }
}
